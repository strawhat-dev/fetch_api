{
  "name": "fetched-api",
  "description": "an api for fetch api.",
  "repository": "github:strawhat-dev/fetched-api",
  "version": "0.3.3",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "types": "./dist/fetched-api.d.ts",
  "module": "./dist/fetched-api.js",
  "exports": {
    ".": "./dist/fetched-api.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "eslint && vite build",
    "build-dts": "eslint && vite build -- dts"
  },
  "peerDependencies": {
    "@types/node": ">=18",
    "type-fest": "^4.x"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.8.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "eslint-define-config": "^1.23.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "terser": "npm:@swc/core",
    "type-fest": "^4.4.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "pnpm": {
    "overrides": {
      "@rollup/plugin-terser>terser": "$terser",
      "vite-plugin-dts>@microsoft/api-extractor": "latest"
    },
    "peerDependencyRules": {
      "allowAny": [
        "terser"
      ]
    }
  }
}
