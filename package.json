{
  "name": "fetched-api",
  "description": "an api for fetch api.",
  "repository": "github:strawhat-dev/fetched-api",
  "version": "0.1.3",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "types": "./dist/fetched-api.d.ts",
  "module": "./dist/fetched-api.js",
  "exports": {
    ".": "./dist/fetched-api.js"
  },
  "files": ["dist"],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "eslint && vite build"
  },
  "peerDependencies": {
    "@types/node": ">=18",
    "type-fest": "^4.x"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@types/node": "^20.5.2",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-define-config": "^1.23.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "terser": "npm:@swc/core",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.2"
  },
  "pnpm": {
    "overrides": {
      "@rollup/plugin-terser>terser": "$terser",
      "vite-plugin-dts>@microsoft/api-extractor": "latest"
    },
    "peerDependencyRules": {
      "allowAny": ["terser"]
    }
  }
}
