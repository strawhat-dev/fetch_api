{
  "name": "fetched-api",
  "description": "an api for fetch api.",
  "repository": "github:strawhat-dev/fetched-api",
  "version": "0.0.8",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "types": "./dist/fetched-api.d.ts",
  "module": "./dist/fetched-api.js",
  "exports": {
    ".": "./dist/fetched-api.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "eslint && vite build"
  },
  "peerDependencies": {
    "@types/node": ">=18",
    "type-fest": "^4.x"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@types/node": "^20.4.4",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.21.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^3.0.0",
    "terser": "npm:@swc/core",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-dts": "^3.3.1",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  },
  "pnpm": {
    "overrides": {
      "@rollup/plugin-terser>terser": "$terser",
      "vite-plugin-dts>@microsoft/api-extractor": "latest"
    },
    "peerDependencyRules": {
      "allowAny": [
        "terser"
      ]
    }
  }
}
