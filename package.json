{
  "name": "fetched-api",
  "description": "api for fetch api.",
  "version": "0.0.2",
  "license": "MIT",
  "type": "module",
  "module": "./dist/fetched-api.js",
  "types": "./dist/fetched-api.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fetched-api.js",
      "types": "./dist/fetched-api.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.15.0"
  },
  "scripts": {
    "build": "eslint && vite build",
